        function GetReport() {
            debugger;

            var theStatusV = $("#theStatus").val();
            var todateV = $("#todate").val();
            var fromdateV = $("#fromdate").val();

            var DataBody = {
                Status: theStatusV,
                FromDate: fromdateV,
                ToDate: todateV
            };

            $.ajax({
                url: "/api/Reports/GetReport",
                data: DataBody,
                type: "POST",
                contentType: "application/x-www-form-urlencoded",
                success:
                    function (data) {
                        debugger;
                        $('#tbl_ReportDetails').find('tbody').children('tr').remove();

                        LoadDTable(data);
                    },
                error:
                    function (err) {
                        debugger;
                        console.log(err);
                    },
                complete: function (e) {
                    debugger;
                    console.log(e);
                }
            });
        }





