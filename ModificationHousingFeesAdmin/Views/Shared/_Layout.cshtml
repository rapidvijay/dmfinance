<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title </title>

    <link href="~/Content/dashboard/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/dashboard/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/css/materialize.min.css" rel="stylesheet" />
    <link href="~/Content/css/custom-styles.css" rel="stylesheet" />
    <link href="~/Content/dashboard/css/styles.css" rel="stylesheet" />
    <link href="~/Content/css/Custom_UI.css" rel="stylesheet" />
    <link href="~/Content/jquery.loadingModal.css" rel="stylesheet" />
   

</head>
<body ng-app="App">

    <div class="container-fluid">
        <header>
            <div class="dm-Logo">
                <a href="#" target="_self" title="Dubai Municipality Portal">
                    <img src="~/Content/image/dm_logo.jpg" alt="DM Logo" />
                </a>
            </div>
            <div class="dg-Logo">
                <a href="http://www.dubai.ae/en/pages/default.aspx" rel="nofollow" target="_blank" title="Dubai Municipality Portal">
                    <img src="~/Content/image/dubai_gov_logo.jpg" alt="DG Logo" />
                </a>
            </div>
        </header>

        <!-- /.container-fluid  start  Header -->
        <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">

            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#"><span>Housing</span> Fees</a>
                <ul class="nav navbar-top-links navbar-right">
                    <li class="fund_icon">
                        <i class="fa fa-home" aria-hidden="true"></i>
                    </li>

                </ul>

                <ul class="nav navbar-top-links navbar-right" style="display:none">
                    <li class="dropdown">
                        <a class="dropdown-toggle count-info" data-toggle="dropdown" href="#">
                            <em class="fa fa-envelope"></em><span class="label label-danger">15</span>
                        </a>
                        <ul class="dropdown-menu dropdown-messages">
                            <li>
                                <div class="dropdown-messages-box">
                                    <a href="profile.html" class="pull-left">
                                        <img alt="image" class="img-circle" src="http://placehold.it/40/30a5ff/fff">
                                    </a>
                                    <div class="message-body">
                                        <small class="pull-right">3 mins ago</small>
                                        <a href="#"><strong>John Doe</strong> commented on <strong>your photo</strong>.</a>
                                        <br /><small class="text-muted">1:24 pm - 25/03/2015</small>
                                    </div>
                                </div>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <div class="dropdown-messages-box">
                                    <a href="profile.html" class="pull-left">
                                        <img alt="image" class="img-circle" src="http://placehold.it/40/30a5ff/fff">
                                    </a>
                                    <div class="message-body">
                                        <small class="pull-right">1 hour ago</small>
                                        <a href="#">New message from <strong>Jane Doe</strong>.</a>
                                        <br /><small class="text-muted">12:27 pm - 25/03/2015</small>
                                    </div>
                                </div>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <div class="all-button">
                                    <a href="#">
                                        <em class="fa fa-inbox"></em> <strong>All Messages</strong>
                                    </a>
                                </div>
                            </li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle count-info" data-toggle="dropdown" href="#">
                            <em class="fa fa-bell"></em><span class="label label-info">5</span>
                        </a>
                        <ul class="dropdown-menu dropdown-alerts">
                            <li>
                                <a href="#">
                                    <div>
                                        <em class="fa fa-envelope"></em> 1 New Message
                                        <span class="pull-right text-muted small">3 mins ago</span>
                                    </div>
                                </a>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <a href="#">
                                    <div>
                                        <em class="fa fa-heart"></em> 12 New Likes
                                        <span class="pull-right text-muted small">4 mins ago</span>
                                    </div>
                                </a>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <a href="#">
                                    <div>
                                        <em class="fa fa-user"></em> 5 New Followers
                                        <span class="pull-right text-muted small">4 mins ago</span>
                                    </div>
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>

        </nav>

        <!--/.sidebar  Start-->
        <div id="sidebar-collapse" class="col-sm-3 col-lg-2 sidebar">
            <div class="profile-sidebar">
                <div class="profile-userpic">
                    <div class="user-responsive">
                        <i class="fa fa-user-o" aria-hidden="true"></i>
                    </div>

                </div>
                <div class="profile-usertitle">
                    <div class="profile-usertitle-name">
                        @if (Common.User != null)
                        {
                            <span> @Common.User.USER_NAME</span>}
                        else { }
                    </div>

                    <div class="profile-usertitle-status"><span class="indicator label-success"></span>Online</div>
                </div>
                <div class="clear"></div>
            </div>
            <div class="divider"></div>
            <form role="search" style="display:none">
                <div class="form-group">
                    <input type="text" class="form-control" placeholder="Search">
                </div>
            </form>
            <ul class="nav menu">
                <li class="@(Request.Url.ToString().Contains("Home/Index") ? "active" : "")"><a href="~/Home/Index"><em class="fa fa-dashboard">&nbsp;</em> Dashboard</a></li>
                <li class="@(ViewContext.RouteData.Values["Action"].ToString() == "housingFeesData" && !Request.Url.ToString().Contains("?reject=") && !Request.Url.ToString().Contains("?completed=")  ? "active" : "")"><a href="~/Home/housingFeesData"><em class="fa fa-pie-chart">&nbsp;</em> Requests (In Progress)</a></li>
                <li class="@(Request.Url.ToString().Contains("?reject=") ? "active" : "")"><a href="~/Home/housingFeesDataRejected?reject=true"><em class="fa fa-pie-chart">&nbsp;</em>Rejected Requests.</a></li>
                <li class="@(Request.Url.ToString().Contains("?completed=") ? "active" : "")"><a href="~/Home/housingFeesDataCompleted?completed=true"><em class="fa fa-pie-chart">&nbsp;</em>Completed Requests.</a></li>
                <li class="@(Request.Url.ToString().Contains("/DReports") ? "active" : "")"><a href="~/Home/DReports"><em class="fa fa-pie-chart">&nbsp;</em>Report</a></li>

                <li class="@(Request.Url.ToString().Contains("Users") ? "active" : "")"><a href="~/Users/Index"><em class="fa fa-users">&nbsp;</em>Users</a></li>


                <li style="display:none"><a href="elements.html"><em class="fa fa-toggle-off">&nbsp;</em> UI Elements</a></li>
                <li style="display:none"><a href="panels.html"><em class="fa fa-clone">&nbsp;</em> Alerts &amp; Panels</a></li>
                <li class="parent">
                    <a data-toggle="collapse" href="#sub-item-1">
                        <em class="fa fa-navicon">&nbsp;</em> Setup <span data-toggle="collapse" href="#sub-item-1" class="icon pull-right"><em class="fa fa-plus"></em></span>
                    </a>
                    <ul class="children collapse" id="sub-item-1">
                        <li style="display:none">
                            <a class="" href="#">
                                <span class="fa fa-arrow-right">&nbsp;</span> Sub Item 1
                            </a>
                        </li>

                        <li class="@(Request.Url.ToString().Contains("PropertyTypes") ? "active" : "")"><a href="~/PropertyTypes/Index"><em class="fa fa-arrow-right">&nbsp;</em>Property Types</a></li>

                        <li class="@(Request.Url.ToString().Contains("Areas") ? "active" : "")"><a href="~/Areas/Index"><em class="fa fa-arrow-right">&nbsp;</em>Areas</a></li>

                        <li class="@(Request.Url.ToString().Contains("CommunityPlaces") ? "active" : "")"><a href="~/CommunityPlaces/Index"><em class="fa fa-arrow-right">&nbsp;</em>Community Places</a></li>
                    </ul>
                </li>
                <li><a href="~/Home/Login"><em class="fa fa-power-off">&nbsp;</em> Logout</a></li>

            </ul>
        </div><!--/.sidebar End -->


        @RenderBody()

        <footer style="display:none">
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    <script src="~/Content/dashboard/js/jquery-1.11.1.min.js"></script>
    <script src="~/Content/dashboard/js/bootstrap.min.js"></script>

    <script src="~/Content/dashboard/js/chart.min.js"></script>
    <script src="~/Content/dashboard/js/chart-data.js"></script>

    @*<script src="~/Content/dashboard/js/easypiechart.js"></script>
        <script src="~/Content/dashboard/js/easypiechart-data.js"></script>*@
    @*<script src="~/Content/dashboard/js/bootstrap-datepicker.js"></script>*@
    <script src="~/Content/dashboard/js/custom.js"></script>
    <script src="~/Content/js/jquery.loadingModal.js"></script>

    <link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/gridmvc.min.js")"></script>


    @*<script>
        window.onload = function () {
            var chart1 = document.getElementById("line-chart").getContext("2d");
            window.myLine = new Chart(chart1).Line(lineChartData, {
                responsive: true,
                scaleLineColor: "rgba(0,0,0,.2)",
                scaleGridLineColor: "rgba(0,0,0,.05)",
                scaleFontColor: "#c5c7cc"
            });
        };</script>*@


    <script src="~/Content/slider/jssor.slider.min.js"></script>
    <script src="~/Content/slider/docs.min.js"></script>
    <link href="~/Content/DataTables/css/dataTables.bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.bootstrap.min.js"></script>
    <script src="~/Content/js/handleMasterDetailsPropertyAreaActions.js"></script>
    <script src="~/Content/js/bootbox.min.js"></script>
    <script src="~/Content/js/handleMasterDetailsAreaCommunityPlacesActions.js"></script>

    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-mocks.js"></script>
    <script src="~/Content/js/ui-bootstrap-tpls-2.5.0.min.js"></script>
    <script src="~/Content/js/viewIntegrationDetails.js"></script>


    <script>
        $(function () {
            var hiddenAreaId = "";
            var propertyId = "";
            //####################Edit property Page ####################
            propertyId = $("#hiddenPropertyId").val();
            // initialize the datatables
            if (propertyId != null && propertyId != "") {
                //$("#areas-data-table").dataTable().fnDestroy();
                areaListVM.init();
            }
            //#####################End Edit property Page #####################


            //#####################Edit Arae Page ###########################
            hiddenAreaId = $("#hiddenAreaId").val();
            if (hiddenAreaId != null && hiddenAreaId != "") {
                // initialize the datatables
                if ($.fn.DataTable.isDataTable('#communityPlace-data-table')) {
                    $('#communityPlace-data-table').DataTable().destroy();
                }
                //$("#communityPlace-data-table").dataTable().fnDestroy();
                communityPlaceListVM.init();
            }
            //#####################End Edit Arae Page #########################
        });
    </script>



</body>
</html>
